<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item active me-auto" aria-current="page">Gems</li>
    <%= render '/partials/breadcrumb/search' %>
  </ol>
</nav>
<div class="row row-cols-1 row-cols-md-2 row-cols-lg-3 row-cols-xl-4 g-4">
  <% @gems.each do |gem| %>
    <div class="col">
      <div class="card h-100">
        <div class="card-body">
          <h5 class="card-title"><%= link_to gem.name, gem_path(gem) %></h5>
          <p class="card-text"><%= gem.title %></p>
        </div>
        <div class="card-footer">
          <small class="text-muted"><%= gem.created_at %></small>
        </div>
      </div>
    </div>
  <% end %>
</div>

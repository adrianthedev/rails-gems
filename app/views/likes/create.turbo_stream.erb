<%= turbo_stream.replace dom_id(@gem, :card) do %>
  <%= render '/gems/card', gem: @gem %>
<% end %>
<%= turbo_stream.replace dom_id(@gem, :details) do %>
  <%= render '/gems/details', gem: @gem %>
<% end %>
<%= turbo_stream.replace "profile" do %>
  <%= render_profile(current_user.reload) %>
<% end %>